{
  "extends": ["airbnb", "plugin:react/recommended", "prettier"],
  "plugins": ["jest", "react"],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "jest/globals": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prefer-stateless-function": 0,
    "react/destructuring-assignment": 0
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
  }
}
